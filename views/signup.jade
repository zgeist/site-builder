extends layouts/default

block content
    .box.center-block(ng-controller="UserAuthCtrl")
        h3 Sign up
        form(ng-submit="UserSignUp()")
            md-input-container
                label Username
                input(ng-model="user.username")
            md-input-container
                label Password
                input(ng-model="user.password", type="password")

            md-button.md-raised.md-primary Sign Up